{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "public/vercel-api/sendEmail.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/sendEmail", "dest": "/public/vercel-api/sendEmail.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
